  <update id="updateByPrimaryKeySelective" parameterType="com.example.community_education.Model.TestPaper" >
    update test_paper
    <set >
      <if test="title != null" >
        title = #{title,jdbcType=VARCHAR},
      </if>
      <if test="dates != null" >
        dates = #{dates,jdbcType=TIMESTAMP},
      </if>
      <if test="deleted != null" >
        deleted = #{deleted,jdbcType=INTEGER},
      </if>
    </set>
    where id = #{id,jdbcType=INTEGER}
  </update>
  <update id="updateByPrimaryKey" parameterType="map" >
    update test_paper
    set
      title = #{title,jdbcType=VARCHAR}
    where id = #{id,jdbcType=INTEGER}
  </update>

  <update id="Delete" parameterType="map" >
    update test_paper
    set
      deleted = 1
    where id = #{id,jdbcType=INTEGER}
  </update>
  <update id="ChangeEffect" parameterType="map" >
    update test_paper
    set
      effect = 0
    where id = #{id,jdbcType=INTEGER}
  </update>


  <select id="AllPaper" resultMap="BaseResultMap" parameterType="map">
    select
    <include refid="Base_Column_List" />
    from test_paper
    where deleted = 0
    order by dates DESC
    limit #{NowPage,jdbcType=INTEGER},10
  </select>

  <select id="selectPaper" resultMap="BaseResultMap" parameterType="map">
    select
    <include refid="Base_Column_List" />
    from test_paper
    where deleted = 0 and
    (
    title LIKE #{title,jdbcType=VARCHAR}
    )
  </select>

  <select id="PaperSum" resultType="java.lang.Integer">
    select
    COUNT(*)
    from test_paper where deleted = 0
