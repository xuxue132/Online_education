{"ast":null,"code":"import HeadTitle from \"@/components/MainBody/HeadTitle\";\nexport default {\n  name: \"FileText\",\n  components: {\n    HeadTitle\n  },\n  data() {\n    return {\n      FileInformation: {},\n      Files: []\n    };\n  },\n  created() {\n    this.GetFileInformation();\n    this.Filed();\n  },\n  methods: {\n    //格式化时间\n    formatDate(date) {\n      if (typeof date === 'string') {\n        return date;\n      }\n      let year = date.getFullYear();\n      let month = date.getMonth() + 1;\n      let day = date.getDate();\n      if (month < 10) {\n        month = \"0\" + month;\n      }\n      if (day < 10) {\n        day = \"0\" + day;\n      }\n      return year + \"-\" + month + \"-\" + day;\n    },\n    GetFileInformation() {\n      this.$axios.post('public/AnFileInformation', {\n        id: this.$route.query.id\n      }, {\n        headers: {\n          'Authorization': this.$store.state.Authorization\n        }\n      }).then(resp => {\n        if (resp.status === 200) {\n          this.FileInformation = resp.data.data;\n          this.FileInformation.dates = this.formatDate(new Date(this.FileInformation.dates));\n        }\n      }).catch(resp => {});\n    },\n    Filed() {\n      this.$axios.post('public/TheFile', {\n        fileId: this.$route.query.id\n      }, {\n        headers: {\n          'Authorization': this.$store.state.Authorization\n        }\n      }).then(resp => {\n        if (resp.status === 200) {\n          this.Files = resp.data.data;\n        }\n      }).catch(resp => {});\n    }\n  }\n};","map":{"version":3,"names":["HeadTitle","name","components","data","FileInformation","Files","created","GetFileInformation","Filed","methods","formatDate","date","year","getFullYear","month","getMonth","day","getDate","$axios","post","id","$route","query","headers","$store","state","Authorization","then","resp","status","dates","Date","catch","fileId"],"sources":["src/components/MainBody/File/FileText.vue"],"sourcesContent":["<template>\n    <div class=\"main_body\">\n        <HeadTitle></HeadTitle>\n        <div class=\"head_picture\">\n        </div>\n    \n        <div class=\"content row\">\n            <div class=\"sub_left\">\n                <h2 class=\"title_left flag1\">下载中心</h2>\n                <ul class=\"flag2 sub_menu\">\n                    <li>\n                        <a  title=\"文件列表\" href=\"/FilePage\" class=\"chs\">文件列表</a>\n                    </li>\n        \n                </ul>\n            </div>\n            <div class=\"sub_right\">\n                <div class=\"bread fs14\">\n                    <a href=\"/HomePage\">首页</a>\n                    &nbsp;-&nbsp;\n                    <a href=\"/FilePage\">文件列表</a>\n                    &nbsp;-&nbsp;\n                    <span>正文</span>\n                </div>\n                <div class=\"page_content\">\n                    <div class=\"page_title\">\n                        <h3 class=\"fs24\">{{FileInformation.title}}</h3>\n                    </div>\n                </div>\n                <p class=\"newsDetailOneP\">\n                    日期：{{FileInformation.dates}}\n                </p>\n            \n                <div style=\"margin-top: 50px\">\n                    <div class=\"lists\" >\n                        <ul style=\"list-style-type:none;\" v-for=\"File in Files\">\n                            <li style=\"margin-top: 10px;\">附件【<a :href=\"File.fileurl\" target=\"_blank\">{{File.filename}}</a>】</li>\n                        </ul>\n                    </div>\n                </div>\n          \n                <div class=\"bottoms\"></div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import HeadTitle from \"@/components/MainBody/HeadTitle\";\n    export default {\n        name: \"FileText\",\n        components: {HeadTitle},\n        data(){\n            return{\n                FileInformation:{},\n                Files:[]\n            }\n        },\n        created(){\n          this.GetFileInformation()\n          this.Filed()\n        },\n        methods:{\n            //格式化时间\n            formatDate(date) {\n                if (typeof date === 'string'){\n                    return date\n                }\n\n                let year = date.getFullYear();\n                let month = date.getMonth()+1;\n                let day = date.getDate();\n                if (month < 10) {\n                    month = \"0\" + month;\n                }\n                if (day < 10) {\n                    day = \"0\" + day;\n                }\n                return year + \"-\" + month + \"-\" + day;\n            },\n            GetFileInformation(){\n                this.$axios.post('public/AnFileInformation', {\n                    id: this.$route.query.id\n                }, {\n                    headers: {'Authorization': this.$store.state.Authorization}\n                }).then(resp => {\n                    if (resp.status === 200) {\n                        this.FileInformation = resp.data.data\n                        this.FileInformation.dates = this.formatDate(new Date(this.FileInformation.dates))\n                    }\n                }).catch(resp => {\n                })\n            },\n            Filed(){\n                this.$axios.post('public/TheFile', {\n                    fileId: this.$route.query.id\n                }, {\n                    headers: {'Authorization': this.$store.state.Authorization}\n                }).then(resp => {\n                    if (resp.status === 200) {\n                        this.Files = resp.data.data\n                    }\n                }).catch(resp => {\n                })\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .main_body{\n        margin: 0;\n        padding: 0;\n        height: 100%;\n        width: 100%;\n    }\n    .head_picture{\n        height: 250px;\n        width: 100%;\n        background: url(\"http://rcqmzsxxw.com/rchema/xuexiwang/img/public/head_bg.png\");\n        /*background: url(\"https://api.ixiaowai.cn/gqapi/gqapi.php\");*/\n        background-size: cover;\n    }\n    .sub_left {\n        width: 230px;\n        float: left;\n        position: relative;\n        z-index: 3;\n        margin-top: -80px;\n        background: url(\"https://www.cqut.edu.cn/images/sub_left_bg.png\") top center no-repeat;\n        padding: 0 0 65px;\n        background-size: 100% auto;\n    }\n    .title_left {\n        line-height: 50px;\n        padding: 15px 10px 15px 40px;\n        color: #fff;\n        position: relative;\n        font-weight: normal;\n    }\n    .flag1 {\n        font-size: 30px;\n    }\n    .sub_menu {\n        margin: 0 0 0 20px;\n        background: #fdf4e1;\n        height: 199px;\n    }\n    .content {\n        position: relative;\n        z-index: 2;\n    }\n    \n    .row {\n        width: 1500px;\n        margin: 0 auto;\n    }\n    .flag2 {\n        font-size: 18px;\n    }\n    .sub_menu > li > a {\n        display: block;\n        line-height: 36px;\n        color: #333;\n        padding: 17px 0 16px 30px;\n        position: relative;\n        border: 1px solid #ededed;\n        border-top: 0;\n    }\n    .sub_right {\n        margin: 10px 0 0 250px;\n        background: #FFFFFF;\n    }\n    .page_content {\n        padding: 0 0 0 40px;\n        clear: right;\n        background: #fff;\n    }\n    .page_title {\n        padding: 0 0 0;\n        overflow: hidden;\n    }\n    .page_title h3 {\n        padding: 25px 0;\n        color: #333;\n        text-align: center;\n    }\n    .fs24 {\n        font-size: 27px;\n    }\n    \n    .lists{\n        margin-left: 20px;\n        font-size: 18px;\n    }\n  \n    .bread {\n        color: #a6a6a6;\n        line-height: 40px;\n        text-align: right;\n        padding: 0 20px;\n        background: #f7f7f7;\n        margin-left: 20px;\n    }\n    .fs14 {\n        font-size: 14px;\n    }\n    .chs{\n        background: #f0ad4e;\n        color: #fff;\n        border-color: #F0AD4E;\n    }\n    .images{\n        margin: 40px auto;\n        height: 320px;\n        width: 600px\n    }\n    .pic{\n        height:100%;\n        max-width: 100%;\n        margin: auto\n    }\n    .newsDetailOneP {\n        font-size: 14px;\n        color: #969696;\n        padding-bottom: 17px;\n        border-bottom: 1px solid #d5d9dc;\n        text-align: center;\n    }\n    .bottoms{\n        height: 20px;\n    }\n</style>"],"mappings":"AAgDA,OAAAA,SAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAF;EAAA;EACAG,KAAA;IACA;MACAC,eAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,kBAAA;IACA,KAAAC,KAAA;EACA;EACAC,OAAA;IACA;IACAC,WAAAC,IAAA;MACA,WAAAA,IAAA;QACA,OAAAA,IAAA;MACA;MAEA,IAAAC,IAAA,GAAAD,IAAA,CAAAE,WAAA;MACA,IAAAC,KAAA,GAAAH,IAAA,CAAAI,QAAA;MACA,IAAAC,GAAA,GAAAL,IAAA,CAAAM,OAAA;MACA,IAAAH,KAAA;QACAA,KAAA,SAAAA,KAAA;MACA;MACA,IAAAE,GAAA;QACAA,GAAA,SAAAA,GAAA;MACA;MACA,OAAAJ,IAAA,SAAAE,KAAA,SAAAE,GAAA;IACA;IACAT,mBAAA;MACA,KAAAW,MAAA,CAAAC,IAAA;QACAC,EAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAF;MACA;QACAG,OAAA;UAAA,sBAAAC,MAAA,CAAAC,KAAA,CAAAC;QAAA;MACA,GAAAC,IAAA,CAAAC,IAAA;QACA,IAAAA,IAAA,CAAAC,MAAA;UACA,KAAAzB,eAAA,GAAAwB,IAAA,CAAAzB,IAAA,CAAAA,IAAA;UACA,KAAAC,eAAA,CAAA0B,KAAA,QAAApB,UAAA,KAAAqB,IAAA,MAAA3B,eAAA,CAAA0B,KAAA;QACA;MACA,GAAAE,KAAA,CAAAJ,IAAA,KACA;IACA;IACApB,MAAA;MACA,KAAAU,MAAA,CAAAC,IAAA;QACAc,MAAA,OAAAZ,MAAA,CAAAC,KAAA,CAAAF;MACA;QACAG,OAAA;UAAA,sBAAAC,MAAA,CAAAC,KAAA,CAAAC;QAAA;MACA,GAAAC,IAAA,CAAAC,IAAA;QACA,IAAAA,IAAA,CAAAC,MAAA;UACA,KAAAxB,KAAA,GAAAuB,IAAA,CAAAzB,IAAA,CAAAA,IAAA;QACA;MACA,GAAA6B,KAAA,CAAAJ,IAAA,KACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}