s === 200) {\n          this.CharacterProjects = resp.data.data;\n          for (var i = 0; i < this.CharacterProjects.length; i++) {\n            this.CharacterProjects[i].dates = this.formatDate(new Date(this.CharacterProjects[i].dates));\n          }\n        }\n      }).catch(resp => {});\n    },\n    NewCharacter() {\n      this.$axios.post('public/CharacterProjectNumber', {}, {\n        headers: {\n          'Authorization': this.$store.state.Authorization\n        }\n      }).then(resp => {\n        if (resp.status === 200) {\n          this.number = resp.data.data;\n          this.Character(this.page);\n        }\n      }).catch(resp => {});\n    }\n  }\n};","map":{"version":3,"names":["VueEditor","name","components","data","searchForm","title","area","imageUrl","si","CharacterProjected","CharacterProject","texts","types","CharacterProjects","fileList","url","dialogFormVisible","dialogFormVisible2","number","page","page2","formLabelWidth","created","NewCharacter","methods","Closes","Closes2","$refs","resetFields","Closes1","Add","handleAvatarSuccess","res","file","GetNew","beforeAvatarUpload","isJPG","type","isJPG2","isLt2M","size","$message","error","$axios","post","id","headers","$store","state","Authorization","then","resp","status","images","catch","Posst","message","Posst2","$router","go","Edits","i","Deleted","$confirm","confirmButtonText","cancelButtonText","handle","length","push","dates","formatDate","Date","handleCurrentChange","Character","date","year","getFullYear","month","getMonth","day","getDate","NowPage"],"sources":["src/components/MainBody/Admin/CharacterPage.vue"],"sourcesContent":["<template>\n    <div class=\"main_body\">\n        <div id=\"NoticeManagement-top\">\n            <el-space id=\"NoticeManagement-top-left\">\n                <span class=\"icons\">标题</span>\n                <el-input size=\"small\" v-model=\"searchForm.title\" placeholder=\"请输入标题\"></el-input>\n                <span class=\"icons\">社区</span>\n                <