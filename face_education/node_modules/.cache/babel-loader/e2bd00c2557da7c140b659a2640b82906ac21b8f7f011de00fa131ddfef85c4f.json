s.user.birthday))\n      }, {\n        headers: {\n          'Authorization': this.$store.state.Authorization\n        }\n      }).then(resp => {\n        if (resp.status === 200) {\n          this.ids = 1;\n          this.$router.go(0);\n          this.user.birthday = this.formatDate(new Date(this.user.birthday));\n        }\n      }).catch(resp => {});\n    }\n  },\n  created() {\n    this.getUser();\n  }\n};","map":{"version":3,"names":["name","data","ids","dialogFormVisible","dialogFormVisible1","dialogFormVisible2","user","username","telephone","sex","birthday","email","area","background","job","hobby","integral","roleName","form3","password","NewPassword","NewPassword1","options","value","label","options2","options3","form","NewTelephone","form2","NewEmail","password1","formLabelWidth","rules","required","message","rules1","rules2","methods","close1","$refs","resetFields","close2","close3","ChangeIds","recoverIds","ChangePassword","validate","valid","$axios","post","headers","$store","state","Authorization","then","resp","$message","type","code","catch","formatDate","date","year","getFullYear","month","getMonth","day","getDate","ChangePhone","$router","push","ChangeEmail","getUser","token","status","Date","UpdateUser","go","created"],"sources":["src/components/MainBody/MyInf.vue"],"sourcesContent":["<template>\n    <div class=\"main_body\">\n        <div class=\"head_tit\">\n            <a class=\"head_tit_button\" style=\"margin-left: 25px;line-height: 50px\">基础信息</a>\n            <el-button class=\"head_tit_button\" type=\"text\" @click=\"ChangeIds()\" style=\"color: #4285f4;float: right;margin-right: 25px\" v-if=\"this.ids===1\">编辑</el-button>\n            <el-button type=\"primary\" class=\"head_tit_button\" @click=\"UpdateUser()\" style=\"float: right;margin-right: 25px\" v-if=\"this.ids===2\">保存</el-button>\n            <el-button class=\"head_tit_button\" @click=\"recoverIds()\" style=\"float: right;margin-right: 15px\" v-if=\"ids===2\">取消</el-button>\n