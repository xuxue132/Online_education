         }).then(resp => {\n                    if (resp.status === 200) {\n                        this.number = resp.data.data\n                        this.NewProject(this.page)\n                    }\n                }).catch(resp => {\n                })\n\n            },\n            NewProject(page){\n                this.$axios.post('public/CharacterProject', {\n                    NowPage: page\n                }, {\n                    headers: {'Authorization': this.$store.state.Authorization}\n                }).then(resp => {\n                    if (resp.status === 200) {\n                        this.CharacterProjects = resp.data.data\n                        for (var i=0;i<this.CharacterProjects.length;i++) {\n                            this.CharacterProjects[i].dates = this.formatDate(new Date(this.CharacterProjects[i].dates))\n                        }\n\n                    }\n                }).catch(resp => {\n                })\n            },\n\n        }\n    }\n</script>\n\n<style scoped>\n    .main_body{\n        margin: 0;\n        padding: 0;\n        height: 100%;\n        width: 100%;\n    }\n    .head_picture{\n        height: 250px;\n        width: 100%;\n        background: url(\"http://rcqmzsxxw.com/rchema/xuexiwang/img/public/head_bg.png\");\n        /*background: url(\"https://api.ixiaowai.cn/gqapi/gqapi.php\");*/\n        background-size: cover;\n    }\n    .min_body{\n        width: 1260px;\n        margin: 0 auto;\n    }\n\n    .min_title {\n        font-size: 14px;\n        color: #767676;\n        line-height: 70px;\n    }\n   \n\n    .head_min{\n        width: 100%;\n        margin-top: 50px;\n    }\n    .head_min ul{\n        margin-top: 50px;\n    }\n\n    .head_min_left{\n        float: left;\n        min-width: 222px;\n        height: 172px;\n        max-width: 222px;\n        margin-right: 24px;\n        overflow: hidden;\n        padding: 10px 10px;\n    }\n    .head_min_left img{\n        width: 100%;\n        height: 100%;\n    }\n   \n    .head_min_right{\n        display: inline;\n        width: 70%;\n        float: left;\n        margin-left: 20px;\n        line-height: 40px;\n    }\n    ._project_left {\n        font-size: 14px;\n        color: #363636;\n        margin-right: 10px;\n    }\n    .intro{\n        width:100%;\n        overflow:hidden;\n        text-overflow:ellipsis;\n        white-space:nowrap;\n        word-break:keep-all;\n        font-size: 14px;\n        color: #868686;\n        \n    }\n    \n    .ul_list{\n        border-bottom: 1px solid #d9d9d9;\n        width: 100%;height: 230px\n    }\n\n    ._project_left span i {\n        color: #868686;\n        padding-right: 5px;\n    }\n</style>"],"mappings":";AAmDA,OAAAA,SAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAF;EAAA;EACAG,KAAA;IACA;MACAC,WAAA;MACAC,iBAAA;MACAC,MAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IACAC,WAAAC,IAAA;MACA,WAAAA,IAAA;QACA,OAAAA,IAAA;MACA;MAEA,IAAAC,IAAA,GAAAD,IAAA,CAAAE,WAAA;MACA,IAAAC,KAAA,GAAAH,IAAA,CAAAI,QAAA;MACA,IAAAC,GAAA,GAAAL,IAAA,CAAAM,OAAA;MACA,IAAAH,KAAA;QACAA,KAAA,SAAAA,KAAA;MACA;MACA,IAAAE,GAAA;QACAA,GAAA,SAAAA,GAAA;MACA;MACA,OAAAJ,IAAA,SAAAE,KAAA,SAAAE,GAAA;IACA;IACAE,KAAAC,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAC,IAAA;QACAC,KAAA;UACAJ,EAAA,EAAAA;QACA;MAEA;IACA;IAEAK,oBAAAC,CAAA;MACA,KAAAC,UAAA,CAAAD,CAAA;IACA;IACAjB,QAAA;MACA,KAAAmB,MAAA,CAAAC,IAAA,mCACA;QACAC,OAAA;UAAA,sBAAAC,MAAA,CAAAC,KAAA,CAAAC;QAAA;MACA,GAAAC,IAAA,CAAAC,IAAA;QACA,IAAAA,IAAA,CAAAC,MAAA;UACA,KAAA9B,MAAA,GAAA6B,IAAA,CAAAhC,IAAA,CAAAA,IAAA;UACA,KAAAwB,UAAA,MAAApB,IAAA;QACA;MACA,GAAA8B,KAAA,CAAAF,IAAA,KACA;IAEA;IACAR,WAAApB,IAAA;MACA,KAAAqB,MAAA,CAAAC,IAAA;QACAS,OAAA,EAAA/B;MACA;QACAuB,OAAA;UAAA,sBAAAC,MAAA,CAAAC,KAAA,CAAAC;QAAA;MACA,GAAAC,IAAA,CAAAC,IAAA;QACA,IAAAA,IAAA,CAAAC,MAAA;UACA,KAAA/B,iBAAA,GAAA8B,IAAA,CAAAhC,IAAA,CAAAA,IAAA;UACA,SAAAuB,CAAA,MAAAA,CAAA,QAAArB,iBAAA,CAAAkC,MAAA,EAAAb,CAAA;YACA,KAAArB,iBAAA,CAAAqB,CAAA,EAAAc,KAAA,QAAA7B,UAAA,KAAA8B,IAAA,MAAApC,iBAAA,CAAAqB,CAAA,EAAAc